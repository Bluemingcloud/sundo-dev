# Java class files
*.class

# Generated files
bin/
gen/
out/
.gradle/
.idea/
local.properties
.gradle/
captures/
.externalNativeBuild/
.navigation/
build/
app/build/

# Gradle files
.gradle/
build/
gradle-app.setting/
app/build/
!gradle-wrapper.jar

# Android Studio specific files
.idea/workspace.xml
.idea/libraries
.idea/modules.xml
.idea/.name
.idea/caches
.idea/assetWizardSettings.xml
.idea/libraries
.idea/tasks.xml
.idea/gradle.xml
.idea/dictionaries
.idea/vcs.xml
.idea/jsLibraryMappings.xml

